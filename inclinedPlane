void hello() {
  println(degrees(angle_IP(150,100,150,994,400,994)));
  println(minCoFriction_IP(angle_IP(150,100,150,994,400,994)));

}

float angle_IP(float x1, float y1, float x2, float y2, float x3, float y3) { //Top Left, Bottom Left, Bottom Right
  return atan((y2-y1)/(x3-x2));
}

float minCoFriction_IP(float radians){
  return tan(radians);
}
